<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="src.model.StokTipiKart" table="stok_tip_kart">

		<meta attribute="class-description">
			This class contains the employee detail.
		</meta>

		<id name="id" type="int" column="stoktip_id">
		</id>

		<property name="kodu" column="stoktip_kodu" type="string" />
		<property name="adi" column="stoktip_adi" type="string" />
		<property name="aciklama" column="stoktip_aciklama" type="string" />


	</class>
</hibernate-mapping>