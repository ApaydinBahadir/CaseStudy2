"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="src.model.KdvTipKart" table="kdv_tip_kart">

		<meta attribute="class-description">
			This class contains the employee detail.
		</meta>

		<id name="id" type="int" column="kdvtip_id">
		</id>

		<property name="kodu" column="kdvtip_kodu" type="int" />
